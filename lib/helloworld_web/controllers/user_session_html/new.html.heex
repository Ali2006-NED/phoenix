        const ttl = localStorage.getItem('td-cdn-ttl')

        if (value && ttl && parseInt(ttl, 10) > Date.now()) {
            this.console.log('[watchdog] retrieving last tried CDN: ' + value)
            return value
        }

        this.clearLastTriedCDN()
        return null
